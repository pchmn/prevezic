{
  "name": "@prevezic/web",
  "version": "0.0.0",
  "description": "Web application for Prevezic",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pchmn/prevezic.git"
  },
  "keywords": [
    "react",
    "vite",
    "typescript",
    "template",
    "ghactions",
    "vitest"
  ],
  "author": "pchmn",
  "license": "MIT",
  "homepage": "https://github.com/pchmn/prevezic",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:ci": "tsc && vite build && cp 404.html dist/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage",
    "serve": "vite preview",
    "prepare": "husky install && rm -rf .git/hooks && ln -s ../.husky .git/hooks",
    "lint": "eslint --cache --fix ./src"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/node": "^18.6.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "c8": "^7.12.0",
    "happy-dom": "^6.0.4",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vite-plugin-pwa": "^0.13.1",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.21.0"
  }
}
